{
  "name": "micovi/laravel-sendy",
  "version": "0.1.0",
  "description": "Service provider for Sendy API in PHP Laravel 5+",
  "license": "MIT",
  "keywords": [
    "laravel",
    "sendy"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Ovidiu Miclea",
      "email": "micleaovidiul@gmail.com",
      "homepage": "https://ovidiumiclea.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/Micovi/laravel-sendy",
  "require": {
    "php": ">=7.0",
    "illuminate/support": "^5.4"
  },
  "require-dev": {
    "orchestra/testbench": "~3.7.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Micovi\\LaravelSendy\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Micovi\\LaravelSendy\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Micovi\\LaravelSendy\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
